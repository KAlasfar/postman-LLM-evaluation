		{
			"name": "TEST2_ِAnimal Story",
			"event": [
				{
					"listen": "test",
					"script": {
						"exec": [
							"// ===============================\r",
							"// 1) Extract Answer From LLM #1\r",
							"// ===============================\r",
							"let result;\r",
							"try {\r",
							"    result = pm.response.json();\r",
							"} catch (e) {\r",
							"    result = {};\r",
							"}\r",
							"\r",
							"let promptText = \"Tell me a story of an animal that has ears in shape of a triangle and does meow.\";\r",
							"let answer = \"\";\r",
							"\r",
							"// Safe extraction\r",
							"if (result && result.choices && result.choices[0] && result.choices[0].message) {\r",
							"    answer = result.choices[0].message.content;\r",
							"} else {\r",
							"    answer = \"No answer returned\";\r",
							"}\r",
							"\r",
							"// ===============================\r",
							"// 2) Send Request To Evaluator LLM\r",
							"// ===============================\r",
							"pm.sendRequest({\r",
							"    url: \"https://openrouter.ai/api/v1/chat/completions\",\r",
							"    method: \"POST\",\r",
							"\r",
							"    header: {\r",
							"        \"Content-Type\": \"application/json\",\r",
							"        \"Authorization\": `Bearer ${pm.environment.get(\"OPENROUTER_API_KEY\")}`\r",
							"    },\r",
							"\r",
							"    body: {\r",
							"        mode: \"raw\",\r",
							"        raw: JSON.stringify({\r",
							"            model: \"meta-llama/llama-3.3-70b-instruct\",\r",
							"            messages: [\r",
							"                {\r",
							"                    role: \"system\",\r",
							"                    content: \"You are an evaluator AI. Reply with ONLY: CORRECT or WRONG.\"\r",
							"                },\r",
							"                {\r",
							"                    role: \"user\",\r",
							"                    content: `Prompt: ${promptText}\\nAnswer: ${answer}`\r",
							"                }\r",
							"            ]\r",
							"        })\r",
							"    }\r",
							"\r",
							"}, (err, res) => {\r",
							"\r",
							"    // ===============================\r",
							"    // 3) Parse Evaluator JSON Safely\r",
							"    // ===============================\r",
							"    let judgeJson;\r",
							"    try {\r",
							"        judgeJson = res.json();\r",
							"    } catch (e) {\r",
							"        judgeJson = {};\r",
							"    }\r",
							"\r",
							"    let evaluation = \"WRONG\";\r",
							"\r",
							"    if (judgeJson.choices && judgeJson.choices[0] && judgeJson.choices[0].message) {\r",
							"        evaluation = judgeJson.choices[0].message.content.trim();\r",
							"    }\r",
							"\r",
							"    // ===============================\r",
							"    // 4) Basic Test (PASS / FAIL)\r",
							"    // ===============================\r",
							"    pm.test(\"Evaluation Result\", function () {\r",
							"        pm.expect(evaluation).to.be.oneOf([\"CORRECT\", \"WRONG\"]);\r",
							"    });\r",
							"\r",
							"    // ===============================\r",
							"    // 5) LLM Report (Appears in Runner)\r",
							"    // ===============================\r",
							"    pm.test(`Prompt: ${promptText}`, () => pm.expect(true).to.be.true);\r",
							"    pm.test(`Answer: ${answer}`, () => pm.expect(true).to.be.true);\r",
							"    pm.test(`Evaluation: ${evaluation}`, () => pm.expect(true).to.be.true);\r",
							"});\r",
							""
						],
						"type": "text/javascript",
						"packages": {},
						"requests": {}
					}
				}
			],
