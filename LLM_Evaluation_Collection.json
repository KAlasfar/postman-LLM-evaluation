		{
			"name": "TEST1_City",
			"event": [
				{
					"listen": "test",
					"script": {
						"exec": [
							"let result;\r",
							"try { result = pm.response.json(); } catch (e) { result = {}; }\r",
							"\r",
							"let promptText = \"Which city is the capital of the country where the Eiffel Tower is located?\";\r",
							"let answer = result?.choices?.[0]?.message?.content || \"No answer returned\";\r",
							"\r",
							"pm.sendRequest({\r",
							"    url: \"https://openrouter.ai/api/v1/chat/completions\",\r",
							"    method: \"POST\",\r",
							"    header: {\r",
							"        \"Content-Type\": \"application/json\",\r",
							"        \"Authorization\": `Bearer ${pm.environment.get(\"OPENROUTER_API_KEY\")}`\r",
							"    },\r",
							"    body: {\r",
							"        mode: \"raw\",\r",
							"        raw: JSON.stringify({\r",
							"            model: \"meta-llama/llama-3.3-70b-instruct\",\r",
							"            messages: [\r",
							"                { role: \"system\", content: \"You are an evaluator AI. Reply ONLY: CORRECT or WRONG.\" },\r",
							"                { role: \"user\", content: `Prompt: ${promptText}\\nAnswer: ${answer}` }\r",
							"            ]\r",
							"        })\r",
							"    }\r",
							"}, (err, res) => {\r",
							"\r",
							"    let evalJson;\r",
							"    try { evalJson = res.json(); } catch (e) { evalJson = {}; }\r",
							"\r",
							"    let evaluation = evalJson?.choices?.[0]?.message?.content?.trim() || \"WRONG\";\r",
							"\r",
							"    pm.test(\"Evaluation Result\", function () {\r",
							"        pm.expect(evaluation).to.be.oneOf([\"CORRECT\", \"WRONG\"]);\r",
							"    });\r",
							"\r",
							"    // ⭐ Save only CORRECT or WRONG\r",
							"    pm.environment.set(\"evaluation\", evaluation);\r",
							"\r",
							"    pm.test(`Prompt: ${promptText}`, () => pm.expect(true).to.be.true);\r",
							"    pm.test(`Answer: ${answer}`, () => pm.expect(true).to.be.true);\r",
							"    pm.test(`Evaluation: ${evaluation}`, () => pm.expect(true).to.be.true);\r",
							"});\r",
							""
						],
						"type": "text/javascript",
						"packages": {},
						"requests": {}
					}
				}
			],
